{
  "name": "ngx-flexible-layout",
  "version": "19.0.0",
  "exports": {
    "./mq": {
      "sass": "./core/sass/_layout-bp.scss"
    },
    "./_mq": {
      "sass": "./core/sass/_layout-bp.scss"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout.mjs"
    },
    "./_private-utils": {
      "types": "./_private-utils/index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout-_private-utils.mjs"
    },
    "./core": {
      "types": "./core/index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout-core.mjs"
    },
    "./extended": {
      "types": "./extended/index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout-extended.mjs"
    },
    "./flex": {
      "types": "./flex/index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout-flex.mjs"
    },
    "./grid": {
      "types": "./grid/index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout-grid.mjs"
    },
    "./server": {
      "types": "./server/index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout-server.mjs"
    },
    "./_private-utils/testing": {
      "types": "./_private-utils/testing/index.d.ts",
      "default": "./fesm2022/ngx-flexible-layout-_private-utils-testing.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtc10005/ngx-flex-layout.git"
  },
  "keywords": [
    "angular",
    "flex-layout",
    "flexbox css",
    "media query",
    "breakpoints"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jtc10005/ngx-flex-layout/issues"
  },
  "homepage": "https://github.com/jtc10005/ngx-flex-layout#readme",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/cdk": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "rxjs": "^6.5.3 || ^7.4.0"
  },
  "type": "module",
  "module": "fesm2022/ngx-flexible-layout.mjs",
  "typings": "index.d.ts",
  "sideEffects": false
}